<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns = "http://www.w3.org/ns/widgets"
    xmlns:gap = "http://phonegap.com/ns/1.0"
    id        = "com.miapp.movil"
    versionCode = "1"
    version   = "1.0.1">

<name>Bikecoin</name>
<description>
Desarrollo personalizado Jquery
</description>
	<author href="https://wordpressreact.com" email="jquery@wordpressreact.com">
	The best
	</author>
  <preference name="phonegap-version" value="cli-7.1.0" />
  <preference name="pgb-builder-version" value="2" />
  <engine name="android" spec="7.0.0" />
    <access origin="*"/>
    <gap:plugin name="cordova-plugin-whitelist"  source="npm" />
    <gap:plugin name="cordova-plugin-device-motion"   source="npm" />
    <gap:plugin name="cordova-plugin-device-orientation"   source="npm" />
    <gap:plugin name="cordova-plugin-splashscreen" source="npm"  />
    <gap:plugin name="cordova-plugin-inappbrowser" source="npm" />
    <gap:plugin name="cordova-plugin-whitelist"   source="npm" />
    <gap:plugin name="cordova-plugin-geolocation"  source="npm" />


    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
  <preference name="fullscreen" value="false" />
  <preference name="prerendered-icon" value="true" />
  <content src="index.html"/>
  <platform name="android">
    <!-- <icon src="hdpi.png" qualifier="hdpi"/>
    <icon src="xdpi.png" qualifier="xdpi"/> -->
    <allow-intent href="market:*" />
  </platform>
  <preference name="SplashScreenDelay" value="5000" />
  <preference name="AutoHideSplashScreen" value="true" />

</widget>
